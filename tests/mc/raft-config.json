{
    "run_config":{"listen_address": "ptcp:6666:127.0.0.1",
		  "max_threads_per_proc" : 2,
		  "search_strategy": "mc_search_strategy_breadth",
		  "max_search_depth": 10},

    "model_check_setup" : {"command":["raft-setup.sh"]},
    
    "model_check_execute": [
	{"raft1":{
            "command":["raft-driver", "--unixctl=raft1.ctl",
		       "--mcaddr=tcp:127.0.0.1:6666", "s1.db"],
	    "failure_inject":true}},
	{"raft2":{
            "command":["raft-driver", "--unixctl=raft2.ctl",
		       "--mcaddr=tcp:127.0.0.1:6666", "s2.db"],
	    "failure_inject":true}},
	{"raft3":{
            "command":["raft-driver", "--unixctl=raft3.ctl",
		       "--mcaddr=tcp:127.0.0.1:6666", "s3.db"],
	    "failure_inject":true}},
	{"client1":{
            "command":["raft-client", "raft1.ctl", "tcp:127.0.0.1:6666", "commands"],
	    "failure_inject":false}}
    ],

    "model_check_cleanup" : {"command":["raft-cleanup", "s1.db", "s2.db", "s3.db"]}
}
